<?xml version="1.0" encoding="utf-8" ?>
<Patch>	

<Operation Class="PatchOperationFindMod">
    <mods>
		<li>FAE-Facial Animations Expanded</li>
    </mods>
	<match Class="PatchOperationSequence">
		<operations>
		
			<li Class="PatchOperationConditional">
			<xpath>Defs/GeneDef[defName="Nose_ShortSnout"]/renderNodeProperties</xpath>
				<match Class="PatchOperationReplace">
					<xpath>Defs/GeneDef[defName="Nose_ShortSnout"]/renderNodeProperties</xpath>
					<value>
						<graphicData>
						  <graphicPath>Things/Pawn/Humanlike/HeadAttachments/ShortSnout/ShortSnout</graphicPath>
						  <useSkinShader>true</useSkinShader>
						  <layer>PostHair</layer>
						</graphicData>
					</value>
				</match>
			</li>

			<li Class="PatchOperationConditional">
			<xpath>Defs/GeneDef[defName="Cheeks_Fluffy"]/renderNodeProperties</xpath>
				<match Class="PatchOperationReplace">
					<xpath>Defs/GeneDef[defName="Cheeks_Fluffy"]/renderNodeProperties</xpath>
					<value>
						<graphicData>
						  <graphicPath>Things/Pawn/Humanlike/HeadAttachments/FluffyCheeks/FluffyCheeks</graphicPath>
						  <useSkinShader>true</useSkinShader>
						  <layer>PostHair</layer>
						</graphicData>
					</value>
				</match>
			</li>
			
			<li Class="PatchOperationConditional">
			<xpath>Defs/GeneDef[defName="Large_Cranium"]/renderNodeProperties</xpath>
				<match Class="PatchOperationReplace">
					<xpath>Defs/GeneDef[defName="Large_Cranium"]/renderNodeProperties</xpath>
					<value>
						<graphicData>
						  <graphicPath>Things/Pawn/Humanlike/HeadAttachments/LargeCranium/LargeCranium</graphicPath>
						  <useSkinShader>true</useSkinShader>
						  <layer>PostHair</layer>
						</graphicData>
					</value>
				</match>
			</li>
			
			<li Class="PatchOperationConditional">
			<xpath>Defs/GeneDef[defName="Face_Gaunt"]/renderNodeProperties</xpath>
				<match Class="PatchOperationReplace">
					<xpath>Defs/GeneDef[defName="Face_Gaunt"]/renderNodeProperties</xpath>
					<value>
						<graphicData>
						  <graphicPath>Things/Pawn/Humanlike/HeadAttachments/GauntFace/GauntFace</graphicPath>
						  <useSkinShader>true</useSkinShader>
						  <layer>PostHair</layer>
						</graphicData>
					</value>
				</match>
			</li>

			<li Class="PatchOperationConditional">
			<xpath>Defs/GeneDef[defName="Cheeks_FacialGills"]/renderNodeProperties</xpath>
				<match Class="PatchOperationReplace">
					<xpath>Defs/GeneDef[defName="Cheeks_FacialGills"]/renderNodeProperties</xpath>
					<value>
						<graphicData>
						  <graphicPath>Things/Pawn/Humanlike/HeadAttachments/FacialGills/FacialGills</graphicPath>
						  <useSkinShader>true</useSkinShader>
						  <layer>PostHair</layer>
						</graphicData>
					</value>
				</match>
			</li>

		</operations>
	</match>
</Operation>

</Patch>