									<value>
										<costList>
											<ZF_BlocksMudstone>4</ZF_BlocksMudstone>
										</costList>
									</value>
								</li>

								</operations>
							</match>
						</li>
	
					</operations>
				</match>
			</li>

			<li Class="PatchOperationFindMod">
