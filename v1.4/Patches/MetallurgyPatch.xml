<?xml version="1.0" encoding="utf-8" ?>

<Patch>		
			
	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>HMC Vile's Metallurgy</li>
	</mods>
		<match Class="PatchOperationSequence">
		<operations>
		
		<li Class="PatchOperationReplace">
			<xpath>Defs/ArchitectSense.DesignationSubCategoryDef[defName="SubCategory_Milling"]</xpath>
			<value>
				<ArchitectSense.DesignationSubCategoryDef>
					<defName>SubCategory_Milling</defName>
					<label>Milling</label>
					<description>Grinding and smashing stuff into smaller bits.</description>
					<defNames>
						<li>SK_Windmill</li>
						<li>StampMill</li>
					</defNames>
					<designationCategory>Production</designationCategory>
					<debug>false</debug>
				</ArchitectSense.DesignationSubCategoryDef>
			</value>
		</li>
	
		</operations>
		</match>
		<nomatch Class="PatchOperationSequence">
		<operations>
		
		<li Class="PatchOperationAdd">
			<xpath>Defs</xpath>
			<value>
				<ArchitectSense.DesignationSubCategoryDef>
					<defName>SubCategory_Milling</defName>
					<label>Milling</label>
					<description>Grinding and smashing stuff into smaller bits.</description>
					<defNames>
						<li>SK_Windmill</li>
					</defNames>
					<designationCategory>Production</designationCategory>
					<debug>false</debug>
				</ArchitectSense.DesignationSubCategoryDef>
			</value>
		</li>
	
		</operations>
		</nomatch>
	</Operation>
			
</Patch>