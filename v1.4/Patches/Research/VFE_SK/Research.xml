                <value>
                    <tab>Buildings_SK</tab>
                </value>
            </li>
			<li Class="PatchOperationConditional">
				<xpath>/Defs/ResearchProjectDef[defName="MF_BasicFurniture"]/modExtensions/li[@Class="ResearchTreeSK.ResearchTreeSKModExtension"]</xpath>
				<nomatch Class="PatchOperationConditional">
					<xpath>/Defs/ResearchProjectDef[defName="MF_BasicFurniture"]/modExtensions</xpath>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/ResearchProjectDef[defName="MF_BasicFurniture"]/modExtensions</xpath>
						<value>
							<li Class="ResearchTreeSK.ResearchTreeSKModExtension"></li>
						</value>
					</match>
					<nomatch Class="PatchOperationAdd">
						<xpath>/Defs/ResearchProjectDef[defName="MF_BasicFurniture"]</xpath>
						<value>
						<modExtensions>
							<li Class="ResearchTreeSK.ResearchTreeSKModExtension"></li>
