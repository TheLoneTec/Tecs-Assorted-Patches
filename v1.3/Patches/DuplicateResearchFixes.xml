<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!--HSK-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Utility Columns SK</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

			<li Class="PatchOperationConditional">
				<xpath>/Defs/ThingDef[defName="GraveColumnMod"]/researchPrerequisites/li[text()="ComplexFurniture"]</xpath>
				<match Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="GraveColumnMod"]/researchPrerequisites/li[text()="ComplexFurniture"]</xpath>
				</match>
			</li>

			</operations>
		</match>
		
	</Operation>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Gloomy Decor Pack</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

			<li Class="PatchOperationConditional">
				<xpath>/Defs/ThingDef[defName="RGK_EndTableWithLamp"]/researchPrerequisites/li[text()="Light_C1"]</xpath>
				<match Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="RGK_EndTableWithLamp"]/researchPrerequisites/li[text()="Light_C1"]</xpath>
				</match>
			</li>

			<li Class="PatchOperationConditional">
				<xpath>/Defs/ThingDef[defName="RGK_EndTableWithLampE"]/researchPrerequisites/li[text()="Light_C2"]</xpath>
				<match Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="RGK_EndTableWithLampE"]/researchPrerequisites/li[text()="Light_C2"]</xpath>
				</match>
			</li>

			</operations>
		</match>
		
	</Operation>
	
</Patch>