<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>A Dog Said... Animal Prosthetics</li>
	</mods>
	<match Class="PatchOperationSequence">
		<operations>

            <li Class="PatchOperationReplace">
                <xpath>Defs/ResearchProjectDef[defName="Animal_D1"]/researchViewX</xpath>
                <value>
                    <researchViewX>17.00</researchViewX>
                </value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>Defs/ResearchProjectDef[defName="Animal_D1"]/researchViewY</xpath>
                <value>
                    <researchViewY>51.00</researchViewY>
                </value>
            </li>

			<li Class="PatchOperationRemove">
				<xpath>/Defs/ResearchProjectDef[defName="Animal_C1"]/prerequisites/li[text()="Animal_A2"]</xpath>
			</li>

		</operations>
		</match>
    </Operation>	
</Patch>	