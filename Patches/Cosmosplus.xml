<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Cosmos+</li>
	</mods>
		<match Class="PatchOperationSequence">
			<operations>
		
			<li Class="PatchOperationAdd">
				<xpath>Defs</xpath>
				<value>
					<ArchitectSense.DesignationSubCategoryDef>
						<defName>SubCategory_CosmosPlus</defName>
						<label>Cosmos</label>
						<description>Everything you need to launch and maintain satellites.</description>
						<defNames>
							<li>Cosmos_CenterNovax</li>
							<li>Cosmos_CargoBeacon</li>
							<li>Cosmos_StationHive</li>
						</defNames>
						<designationCategory>Misc</designationCategory>
						<debug>false</debug>
					</ArchitectSense.DesignationSubCategoryDef>
				</value>
			</li>

			</operations>
		</match>
    </Operation>
	
</Patch>